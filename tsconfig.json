{
  "compilerOptions": {
    "esModuleInterop": true,                             /*This is for "path" module import*/
    "baseUrl": ".",                                      /* Specify the base directory to resolve non-relative module names. */
    "paths": {     
      "@pages/common/*": ["pageFactory/pageRepository/commonpages/*"],
      "@pages/romania/*": ["pageFactory/pageRepository/romaniapages/*"],
      "@pages/us/*": ["pageFactory/pageRepository/USpages/*"],                                 
           /* Specify a set of entries that re-map imports to additional lookup locations. */
      "@pages/*":["pageFactory/pageRepository/*"],
      "@objects/*":["pageFactory/objectRepository/*"],
      "@lib/*":["lib/*"],
      
    } 
       // Enable downlevelIteration
  }
}